//function:





int birthdayCakeCandles(int candles_count, int* candles) {
    int i,j,tallest=0,talcount=0;
    for(i=0;i<candles_count;i++){
        if(candles[i]>tallest){
            tallest=candles[i];
        }
    }
    for(i=0;i<candles_count;i++){
        if(candles[i]==tallest){
            talcount++;
        }
    }
    return talcount;
}
